{
	"multipart": [
		{ "apply": { "model": "bcp:block/empty" } },
		{ "when": { "part": 3, "facing": "north", "hinge": "left", "open": false}, 
			"apply": {"model": "bcp:block/test_double_size_door_lower"} },
		{ "when": { "part": 3, "facing": "north", "hinge": "left", "open": true}, 
			"apply": {"model": "bcp:block/test_double_size_door_lower_hinge", "y": 270} },
		{ "when": { "part": 3, "facing": "north", "hinge": "right", "open": false}, 
			"apply": {"model": "bcp:block/test_double_size_door_lower_hinge"} },
		{ "when": { "part": 3, "facing": "north", "hinge": "right", "open": true}, 
			"apply": {"model": "bcp:block/test_double_size_door_lower", "y": 90} },

		{ "when": { "part": 3, "facing": "west", "hinge": "left", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_lower", "y": 90} },
		{ "when": { "part": 3, "facing": "west", "hinge": "left", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_lower_hinge"} },
		{ "when": { "part": 3, "facing": "west", "hinge": "right", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_lower_hinge", "y": 90} },
		{ "when": { "part": 3, "facing": "west", "hinge": "right", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_lower", "y": 180} },

		{ "when": { "part": 3, "facing": "south", "hinge": "left", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_lower", "y": 180} },
		{ "when": { "part": 3, "facing": "south", "hinge": "left", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_lower_hinge", "y": 90} },
		{ "when": { "part": 3, "facing": "south", "hinge": "right", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_lower_hinge", "y": 180} },
		{ "when": { "part": 3, "facing": "south", "hinge": "right", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_lower", "y": 270} },

		{ "when": { "part": 3, "facing": "east", "hinge": "left", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_lower", "y": 270} },
		{ "when": { "part": 3, "facing": "east", "hinge": "left", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_lower_hinge", "y": 180} },
		{ "when": { "part": 3, "facing": "east", "hinge": "right", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_lower_hinge", "y": 270} },
		{ "when": { "part": 3, "facing": "east", "hinge": "right", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_lower"} },

		{ "when": { "part": 1, "facing": "north", "hinge": "left", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_upper"} },
		{ "when": { "part": 1, "facing": "north", "hinge": "left", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_upper_hinge", "y": 270} },
		{ "when": { "part": 1, "facing": "north", "hinge": "right", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_upper_hinge"} },
		{ "when": { "part": 1, "facing": "north", "hinge": "right", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_upper", "y": 90} },

		{ "when": { "part": 1, "facing": "west", "hinge": "left", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_upper", "y": 90} },
		{ "when": { "part": 1, "facing": "west", "hinge": "left", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_upper_hinge"} },
		{ "when": { "part": 1, "facing": "west", "hinge": "right", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_upper_hinge", "y": 90} },
		{ "when": { "part": 1, "facing": "west", "hinge": "right", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_upper", "y": 180} },

		{ "when": { "part": 1, "facing": "south", "hinge": "left", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_upper", "y": 180} },
		{ "when": { "part": 1, "facing": "south", "hinge": "left", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_upper_hinge", "y": 90} },
		{ "when": { "part": 1, "facing": "south", "hinge": "right", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_upper_hinge", "y": 180} },
		{ "when": { "part": 1, "facing": "south", "hinge": "right", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_upper", "y": 270} },

		{ "when": { "part": 1, "facing": "east", "hinge": "left", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_upper", "y": 270} },
		{ "when": { "part": 1, "facing": "east", "hinge": "left", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_upper_hinge", "y": 180} },
		{ "when": { "part": 1, "facing": "east", "hinge": "right", "open": false},
			"apply": {"model": "bcp:block/test_double_size_door_upper_hinge", "y": 270} },
		{ "when": { "part": 1, "facing": "east", "hinge": "right", "open": true},
			"apply": {"model": "bcp:block/test_double_size_door_upper"} }
	]
}
